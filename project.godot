config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "MetaBoyData",
"language": "GDScript",
"path": "res://MetaBoy/MetaBoyData.gd"
}, {
"base": "Area2D",
"class": "ShotProjectile",
"language": "GDScript",
"path": "res://MetaBoy/Projectiles/ShotProjectile.gd"
}, {
"base": "KinematicBody2D",
"class": "ThrownProjectile",
"language": "GDScript",
"path": "res://MetaBoy/Projectiles/ThrownProjectile.gd"
} ]
_global_script_class_icons={
"MetaBoyData": "",
"ShotProjectile": "",
"ThrownProjectile": ""
}

[application]

config/name="TowerOfMetababel"
run/main_scene="res://UI/Screens/TitleScreen.tscn"
config/icon="res://game_icon.png"

[autoload]

Globals="*res://Globals.gd"
SceneManager="*res://SceneManager.tscn"
MetaBoyGlobals="*res://MetaBoy/MetaBoyGlobals.gd"
Loopring="*res://addons/godot-loopring-sdk/Loopring.gd"
LoopringGlobals="*res://addons/godot-loopring-sdk/LoopringGlobals.gd"
Stacks="*res://addons/godot-stacks-sdk/Stacks.gd"
StacksGlobals="*res://addons/godot-stacks-sdk/StacksGlobals.gd"

[display]

window/size/width=640
window/size/height=360
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/godot-loopring-sdk/plugin.cfg", "res://addons/godot-stacks-sdk/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

